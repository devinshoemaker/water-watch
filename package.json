{
  "name": "water-watch",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@capacitor/android": "^1.2.0",
    "@capacitor/core": "1.2.0",
    "@capacitor/ios": "^1.2.0",
    "@ionic/react": "^4.11.1",
    "@ionic/react-router": "^4.11.1",
    "@types/jest": "^24.0.19",
    "@types/node": "11.11.3",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.0",
    "firebase": "^6.6.0",
    "firebaseui": "^4.2.0",
    "ionicons": "^4.6.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-firebaseui": "^4.0.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "rxfire": "^3.8.0",
    "rxjs": "^6.5.3",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "prettier --config .prettierrc --check 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "lint:write": "prettier --config .prettierrc --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "e2e": "concurrently \"cypress run\" --kill-others \"BROWSER=none react-scripts start\" --success first",
    "e2e:watch": "concurrently \"cypress open\" --kill-others \"BROWSER=none react-scripts start\" --success first"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "An app to help you keep track of your water intake",
  "devDependencies": {
    "@capacitor/cli": "1.2.0",
    "@testing-library/react": "^9.3.0",
    "concurrently": "^4.1.2",
    "cypress": "^3.4.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  }
}
