{
  "name": "water-watch",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@capacitor/android": "^1.4.0",
    "@capacitor/core": "^1.4.0",
    "@capacitor/ios": "^1.4.0",
    "@ionic/react": "^4.11.7",
    "@ionic/react-router": "^4.11.7",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.17",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "firebase": "^7.6.0",
    "firebaseui": "^4.3.0",
    "ionicons": "^4.6.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-firebaseui": "^4.0.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll",
    "eject": "react-scripts eject",
    "format": "prettier --config .prettierrc --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "format:check": "prettier --config .prettierrc --check 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "e2e": "concurrently \"cypress run\" --kill-others \"BROWSER=none react-scripts start\" --success first",
    "e2e:watch": "concurrently \"cypress open\" --kill-others \"BROWSER=none react-scripts start\" --success first"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "An app to help you keep track of your water intake",
  "devDependencies": {
    "@capacitor/cli": "^1.4.0",
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "concurrently": "^5.0.1",
    "cypress": "^3.8.0",
    "eslint-plugin-cypress": "^2.8.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  }
}
